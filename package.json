  "bugs": {
    "url": "https://github.com/developersdo/opensource-data/issues"
  },
  "homepage": "https://github.com/developersdo/opensource-data#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint; pretty-quick --staged"
    }
  }
}
